<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="547.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PropertiesController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" />
      <Label layoutX="60.0" layoutY="20.0" text="choose wall type" />
      <ComboBox fx:id="ComboBox_Wall" layoutX="34.0" layoutY="60.0" onAction="#comboBoxWallSelctionChanged" prefWidth="150.0" />
      <Canvas fx:id="canvas_walltype" height="150.0" layoutX="50.0" layoutY="100.0" width="150.0" />
      <ComboBox fx:id="ComboBox_character" layoutX="234.0" layoutY="60.0" onAction="#comboBoxCharacterSelctionChanged" prefWidth="150.0" />
      <Label layoutX="255.0" layoutY="20.0" text="choose character" />
      <Canvas fx:id="canvas_characterType" height="150.0" layoutX="250.0" layoutY="100.0" width="150.0" />
      <Button fx:id="btn_setProperties" layoutX="244.0" layoutY="280.0" mnemonicParsing="false" onAction="#setProperties" text="Set new properties" />
   </children>
</AnchorPane>
